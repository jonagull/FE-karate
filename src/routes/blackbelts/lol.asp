                <% Dim tfm_DescField Function tfm_regexpReplace(originalString,searchword,before,after) Set
                    tfm_regexp=New RegExp With tfm_regexp .Pattern="(" & searchword & ")" .IgnoreCase=True .Global=True
                    End With Set matches=tfm_regexp.Execute(originalString) For each match in matches
                    index=match.FirstIndex + 1 temp=Left(originalString, index -1) originalString=temp &
                    Replace(originalString, match.Value, before & match.Value & after, index, 1) Next
                    tfm_regexpReplace=originalString End Function Function
                    tfm_AccentedSearchWords(tfm_DescField,searchfield,before,after) If ( tfm_DescField <> "") Then

                    If CStr(searchfield) <> "" Then

                        tfm_DescField = Replace(tfm_DescField,chr(10),"<br>")

                        For i = 0 to ubound(splitField)

                        tfm_DescField = tfm_regexpReplace(tfm_DescField,splitField(i), before, after)

                        Next

                        End If

                        End If

                        tfm_AccentedSearchWords = tfm_DescField

                        End Function

